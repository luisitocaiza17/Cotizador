+		if(cantonId.length() > 0)
+			valoresWhereQuery +=" AND c.cantonId =:cantonId ";
+		
+		stringQuery = stringQuery + valoresWhereQuery + " ORDER BY c.cultivoNombre, c.provinciaId, c.cantonId, c.tipoCalculoId ";
