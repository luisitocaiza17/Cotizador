		                    valorSubTotal = Math.rint(valorBase*100)/100;
		                }
		                else if(variable.getNombre().equals("IVA")){
		                    valorIva = Math.rint(Double.parseDouble(variable.getValor())*valorSubTotal/100*100)/100;
		                    cotizacion.setImpIva(valorIva);
